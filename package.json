    "@typescript-eslint/parser": "^5.59.5",
    "@wdio/types": "^8.1.2",
    "ci": "^2.2.0",
    "eslint": "^8.33.0",
    "jest": "^29.3.1",
    "lodash": "^4.17.21",
    "mocha": "^10.2.0",
    "png-js": "^1.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3",
    "wd": "^1.14.0",
    "wdio-wait-for": "^2.2.6"
  },
